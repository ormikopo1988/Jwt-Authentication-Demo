{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\SQLExpress;Database=JwtAuthDemoDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "JwtIssuerOptions": {
    "Issuer": "webApi",
    "Audience": "https://localhost:44364/" // whereever your aspnetcore 2.1 api project is located
  },
  "LinkedInOAuthSettings": {
    "ClientId": "<your-linkedin-registered-client-app-id>",
    "ClientSecret": "<your-linkedin-registered-client-app-secret>"
  }
}